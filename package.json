{
  "name": "@anasroud/comp-library",
  "version": "0.0.5",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "sideEffects": false,
  "types": "./dist/lib/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint:css": "stylelint --config stylelint.config.js \"src/**/*.scss\"",
    "build": "rollup -c",
    "lint:ts": "eslint . --ignore-pattern \"storybook-static/*\" --ignore-pattern \"coverage/*\"",
    "lint:all": "npm run lint:ts && npm run lint:css",
    "format": "prettier --write .",
    "prepare": "husky prepare"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.scss": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "keywords": [],
  "author": "Anas Roud: Senior Frontend Developer",
  "license": "ISC",
  "description": "This is a component library that is built using React, TypeScript, and Storybook.",
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@storybook/addon-a11y": "^8.5.1",
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/addon-onboarding": "^8.5.1",
    "@storybook/addon-styling-webpack": "^1.0.1",
    "@storybook/addon-themes": "^8.5.1",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/blocks": "^8.5.1",
    "@storybook/react": "^8.5.1",
    "@storybook/react-webpack5": "^8.5.1",
    "@storybook/test": "^8.5.1",
    "@storybook/theming": "^8.5.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.18",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "autoprefixer": "^10.4.12",
    "babel-jest": "^29.7.0",
    "css-loader": "^6.11.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "file-loader": "^6.2.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.2",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prop-types": "^15.8.1",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.34.0",
    "sass-loader": "^16.0.4",
    "storybook": "^8.5.1",
    "storybook-dark-mode": "^4.0.2",
    "stylelint": "^14.8.2",
    "stylelint-config-css-modules": "^4.1.0",
