    "classnames": "^2.5.1",
    "react": "^19.0.0",
    "sass": "^1.83.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anasroud/comp-library.git"
  },
  "bugs": {
    "url": "https://github.com/anasroud/comp-library/issues"
